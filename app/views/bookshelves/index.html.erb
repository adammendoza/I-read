<div class='container'>
  <div class='row'>
<h1 class='mb-5 mx-auto'>Bookshelves</h1>
  
   
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Books</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>

  <% @bookshelves.each do |bookshelf| %>
    <%# <% if bookshelf.account_id == current_account.id %> 
    <tr>
      <td><%= bookshelf.name %></td>
      <td>
        <% (bookshelf.books.pluck :title).each do |title| %>
           <p> <%= title  %> </p>
        <% end %>
      </td> 
      <td><%= link_to "Edit", edit_bookshelf_path(bookshelf)%></td>

    </tr>
    <%# <% end %> 
  <% end %>  
  </tbody>
</table>
</div>
</div>

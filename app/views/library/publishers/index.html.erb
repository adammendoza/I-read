<div class='container'>
  <div class='row'>
   
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Country</th>
      <th scope="col">Books</th>
    </tr>
  </thead>
  <tbody>
  <% @publishers.each do |publisher| %>
    <tr>
      <td><%= publisher.name %></td>
      <td><%= publisher.country %></td>
      <td><%= publisher.books %></td>
    </tr>
  <% end %>  
  </tbody>
</table>
</div>
</div>
